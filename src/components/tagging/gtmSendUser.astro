<script>
  import { $user } from "@/lib/stores/index";

  $user.subscribe((value) => {
    if (value) {
      // @ts-expect-error
      window.customDataLayer = window.customDataLayer || [];
      // @ts-expect-error
      window.customDataLayer.push({ user: value.username });
    }
  });
</script>
